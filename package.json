{
  "name": "@unlockddao/unlockd-protocol",
  "version": "1.0.2",
  "description": "Unlockd Protocol smart contracts",
  "files": [
    "contracts",
    "artifacts"
  ],
  "scripts": { 
    "run-env": "yarn && tail -f /dev/null",
    "hardhat": "hardhat",
    "hardhat:node": "hardhat node",
    "hardhat:localhost": "hardhat --network localhost",
    "hardhat:goerli": "hardhat --network goerli",
    "hardhat:main": "hardhat --network main",
    "size": "npm run compile && npm run hardhat size-contracts",
    "compile": "cross-env SKIP_LOAD=true hardhat compile",
    "test": "npm run compile && TS_NODE_TRANSPILE_ONLY=1 hardhat test ./test/*.spec.ts",
    "test:localhost": "npm run compile && TS_NODE_TRANSPILE_ONLY=1 npm run hardhat:localhost test ./test/*.spec.ts",
    "test:file": "npm run compile && TS_NODE_TRANSPILE_ONLY=1 hardhat test ./test/__setup.spec.ts ./test/${TEST_FILE}",
    "dev:update-abis": "npm run compile && node ./scripts/updateAbis && npm run prettier:abis",
    "prettier:abis": "prettier --write 'abis/**/*.json'",
    "prettier:check": "npx prettier -c 'tasks/**/*.ts' 'contracts/**/*.sol' 'helpers/**/*.ts'  'test/**/*.ts'",
    "prettier:write": "npm run prettier:abis && prettier --write 'tasks/**/*.ts' 'contracts/**/*.sol' 'helpers/**/*.ts'  'test/**/*.ts'",
    "ci:clean": "hardhat clean && rm -rf ./artifacts ./cache ./types",
    "unlockd:hardhat:dev:migration": "npm run compile && npm run hardhat -- unlockd:dev",
    "unlockd:localhost:dev:migration": "npm run compile && npm run hardhat:localhost -- unlockd:dev",
    "unlockd:goerli:mock:migration": "npm run compile && npm run hardhat:goerli -- unlockd:mock",
    "unlockd:localhost:full:migration": "npm run compile && npm run hardhat:localhost -- unlockd:mainnet",
    "unlockd:goerli:full:migration": "npm run compile && npm run hardhat:goerli -- unlockd:mainnet",
    "unlockd:fork:full:migration": "npm run compile && npm run hardhat -- unlockd:mainnet",
    "unlockd:main:full:migration": "npm run compile && npm run hardhat:main -- unlockd:mainnet --verify",
    "goerli:verify": "npm run hardhat:goerli verify:general -- --all --pool Unlockd",
    "goerli:verify:reserves": "npm run hardhat:goerli verify:reserves -- --pool Unlockd",
    "goerli:print-contracts": "npm run hardhat:goerli -- print-contracts",
    "goerli:print-config": "hardhat --network goerli print-config --pool Unlockd",
    "main:verify": "npm run hardhat:main verify:general -- --all --pool Unlockd",
    "main:verify:reserves": "npm run hardhat:main verify:reserves -- --pool Unlockd",
    "main:print-contracts": "npm run hardhat:main -- print-contracts",
    "main:print-config": "hardhat --network main print-config --pool Unlockd",
    "lint:fix": "npm run prettier:write && eslint --fix test/**/*.* tasks/**/*.* helpers/**/*.* unlockd-tasks/**/*.*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged  --pattern 'unlockd-tasks/**/*.ts' --pattern 'contracts/**/*.sol' --pattern 'helpers/**/*.ts'  --pattern 'test/**/*.ts' --pattern 'tasks/**/*.ts'"
    }
  },
  "dependencies": {
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@chainlink/contracts": "0.5.1",
    "@nomiclabs/hardhat-ethers": "2.0.3",
    "@nomiclabs/hardhat-etherscan": "2.1.8",
    "@nomiclabs/hardhat-waffle": "2.0.1",
    "@openzeppelin/contracts": "4.4.1",
    "@openzeppelin/contracts-upgradeable": "4.4.1",
    "@typechain/ethers-v5": "2.0.0",
    "@typechain/hardhat": "1.0.1",
    "@typechain/truffle-v5": "2.0.2",
    "@typechain/web3-v1": "4.0.0",
    "@types/lowdb": "1.0.9",
    "@types/mocha": "7.0.2",
    "bignumber.js": "9.0.0",
    "chai-bignumber": "3.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eth-sig-util": "2.5.3",
    "ethereum-waffle": "3.4.0",
    "ethereumjs-util": "^7.0.0",
    "ethers": "^5.0.19",
    "global-agent": "3.0.0",
    "hardhat": "^2.2.0",
    "hardhat-contract-sizer": "2.5.1",
    "hardhat-dependency-compiler": "1.1.2",
    "hardhat-gas-reporter": "1.0.6",
    "hardhat-storage-layout-diff": "0.0.4",
    "husky": "^4.2.5",
    "lowdb": "1.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.2.0",
    "prettier-plugin-solidity": "^1.0.0-alpha.53",
    "pretty-quick": "^3.1.3",
    "solidity-coverage": "0.7.16",
    "tmp-promise": "3.0.3",
    "ts-generator": "0.1.1",
    "ts-node": "8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typechain": "4.0.3",
    "typescript": "^4.9.4"
  }
}